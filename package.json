{
  "name": "ff-typescript-node",
  "version": "1.0.0",
  "description": "Basic demo of using Typescript on the server side",
  "lib": [
    "es2015"
  ],
  "target": "es6",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts localhost 8080",
    "dev": "nodemon src/index.ts",
    "dev:debug": "nodemon --inspect src/index.ts"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "builtin-status-codes": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.7.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/mongoose": "^5.5.23",
    "@types/node": "^12.7.12",
    "nodemon": "^1.19.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "keywords": []
}
